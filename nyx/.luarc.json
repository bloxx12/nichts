{
	"$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
	"workspace": {
		"library": ["runtime/lua"],
		"checkThirdParty": "Disable"
	},

	"diagnostics": {
		"groupFileStatus": {
			"strict": "Opened",
			"strong": "Opened"
		},
		"groupSeverity": {
			"strong": "Warning",
			"strict": "Warning"
		},
		"unusedLocalExclude": ["_*"],
		"disable": ["luadoc-miss-see-name"]
	}
}
